<h1 class="text-2xl">Import Shipping Details</h1>

<%= form_with url: store_admin_shipping_details_path(current_store_admin), multipart: true, method: :post, local: true do |form| %>
  <div class="form-group">
    <%= form.label :file, "Upload CSV File" %>
    <%= form.file_field :file, class: "input" %>
  </div>

  <div class="form-group">
    <%= form.submit "Upload", class: "button" %>
  </div>
<% end %>
