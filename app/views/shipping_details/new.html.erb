<h1 class="text-2xl">Import Shipping Details</h1>

<%= form_with model: [current_store_admin, @shipping_detail], method: :post do |form| %>
  <div class="form-group">
    <%= form.label :import_file, "Upload CSV File" %>
    <%= form.file_field :import_file, class: "input" %>
  </div>

  <div class="form-group">
    <%= form.submit "Upload", class: "button" %>
  </div>
<% end %>
