<h1>Products</h1>
<% if current_admin %>
  <%= link_to "New Product", new_product_path, data: { turbo_frame: dom_id(Product.new) } %>
<% end %>

<%# <%= turbo_frame_tag Product.new %1> %>

<%= turbo_frame_tag Product.new, class: "grid-container" do %>
    <% @products.each do |product| %>
      <%= render product %>
    <% end %>
<% end %>
